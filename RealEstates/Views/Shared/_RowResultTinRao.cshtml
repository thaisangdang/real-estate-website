@model RealEstates.Models.TinRaoBDS

@if (!string.IsNullOrEmpty(Model.AnhDaiDien))
{
    <img src="@Html.DisplayFor(model => model.AnhDaiDien)" alt="@Html.DisplayFor(model => model.TieuDe)" class="img-thumbnail image-detail">
}
else
{
    <img src="~/Content/Images/AnhDaiDienDuAn/notfound.jpg" alt="" class="img-thumbnail image-detail">
}